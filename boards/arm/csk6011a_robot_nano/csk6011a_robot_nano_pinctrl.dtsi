#include <csk/csk6_pinctrl.dtsi>

#define csk6011a_robot_nano_pinctrl pinctrl \
        __PINCTRL_DEPRECATED__("This node has been deprecated," \
                "please use 'pinctrl' instead of 'csk6011a_robot_nano_pinctrl")

&pinctrl{

        /* UART alternate function */
        pinctrl_uart0_rx_default: uart0_rx_default{
                pinctrls = <UART0_RXD_GPIOA_02>;
        };

        pinctrl_uart0_tx_default: uart0_tx_default{
                pinctrls = <UART0_TXD_GPIOA_03>;
        };

        /* I2C alternate function */
        pinctrl_i2c0_scl_default: i2c0_scl_default{
                pinctrls = <I2C0_SCL_GPIOB_00>;
        };

        pinctrl_i2c0_sda_default: i2c0_sda_default{
                pinctrls = <I2C0_SDA_GPIOB_01>;
        };
        /* SPIC alternate function */
        pinctrl_spi0_cs_default: spi0_cs_default{
                pinctrls = <SPI0_CS_N_GPIOA_20>;
        };

        pinctrl_spi0_miso_default: spi0_miso_default{
                pinctrls = <SPI0_MISO_GPIOA_17>;
        };

        pinctrl_spi0_mosi_default: spi0_mosi_default{
                pinctrls = <SPI0_MOSI_GPIOA_18>;
        };

        pinctrl_spi0_sclk_default: spi0_sclk_default{
                pinctrls = <SPI0_CLK_GPIOA_19>;
        };

        pinctrl_spi1_sclk_default: spi1_sclk_default{
                pinctrls = <SPI1_CLK_GPIOA_20>;
        };

        pinctrl_spi1_mosi_default: spi1_mosi_default{
                pinctrls = <SPI1_MOSI_GPIOA_19>;
        };

        pinctrl_spi1_miso_default: spi1_miso_default{
                pinctrls = <SPI1_MISO_GPIOA_18>;
        };

        pinctrl_spi1_cs_default: spi1_cs_default{
                pinctrls = <SPI1_CS_N_GPIOA_17>;
        };

        pinctrl_pwm2_default: pwm2_default{
                pinctrls = <GPT_PWM_2_GPIOB_06>;
        };

        pinctrl_pwm4_default: pwm4_default{
                pinctrls = <GPT_PWM_4_GPIOA_04>;
        };

        pinctrl_pwm5_default: pwm5_default{
                pinctrls = <GPT_PWM_5_GPIOA_05>;
        };

        pinctrl_pwm6_default: pwm6_default{
                pinctrls = <GPT_PWM_6_GPIOB_02>;
        };

};
